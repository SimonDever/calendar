    "@types/react-modal": "^1.6.2",
    "@types/react-router": "^3.0.8",
    "@types/sequelize": "^4.0.49",
    "babel-polyfill": "^6.22.0",
    "body-parser": "^1.16.1",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "cors": "2.8.3",
    "express": "^4.14.1",
    "express-jwt": "^5.1.0",
